{
  "name": "fitted",
  "version": "0.1.0",
  "private": true,
  "description": "Fitted monorepo",
  "author": "Fitted",
  "license": "UNLICENSED",
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "lint": "eslint \"**/*.{ts,tsx,js,jsx}\" --max-warnings=0",
    "typecheck": "tsc -b",
    "test": "vitest run",
    "dev": "npm run dev --workspaces",
    "dev:api": "npm run dev --workspace @fitted/api",
    "dev:admin": "npm run dev --workspace @fitted/admin",
    "dev:checkout": "npm run dev --workspace @fitted/checkout",
    "build:checkout": "npm run build --workspace @fitted/checkout"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@types/node": "^24.9.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react-swc": "^4.2.0",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vitest": "^4.0.6"
  },
  "dependencies": {
    "lucide-react": "^0.552.0"
  }
}
